{
  "name": "spotify-integrations-demo",
  "version": "1.1.0",
  "description": "A demo app for spotify integrations",
  "main": "server.js",
  "scripts": {
    "build": "webpack --config webpack.prod.config.js",
    "dev": "webpack-dev-server --hot --inline",
    "lint": "eslint src/**",
    "lint:watch": "esw -w src/**",
    "review": "npm run lint && npm test",
    "start": "npm run build && NODE_ENV=production node server.js",
    "test": "jest src/**",
    "test:watch": "jest src/** --watch",
    "test:watchAll": "jest src/** --watchAll",
    "test:coverage:report": "open coverage/lcov-report/index.html"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "array-sort": "^1.0.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cookie-parser": "^1.4.1",
    "css-loader": "^0.28.7",
    "enzyme-adapter-react-16": "^1.0.0",
    "express": "^4.15.5",
    "loadash": "^1.0.0",
    "node-sass": "^4.5.3",
    "prop-types": "^15.6.0",
    "raf": "^3.3.2",
    "raw-loader": "^0.5.1",
    "react": "^0.14.7",
    "react-cards": "^0.2.2",
    "react-dom": "^0.14.7",
    "react-hot-loader": "^1.3.0",
    "react-infinite-grid": "^0.4.0",
    "react-redux": "^4.2.1",
    "react-router": "^2.0.0-rc5",
    "react-router-redux": "^3.0.0",
    "react-transform-hmr": "^1.0.2",
    "redbox-react": "^1.2.2",
    "redux": "^3.2.1",
    "redux-thunk": "^1.0.3",
    "rgx": "^0.2.4",
    "sass-loader": "^6.0.6",
    "sort-arr": "^0.0.2",
    "spotify-web-api-js": "^0.15.0",
    "spotify-web-api-node": "^2.2.0",
    "style-loader": "^0.21.0",
    "webpack": "^3.6.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.1",
    "enzyme": "^3.0.0",
    "eslint": "^4.7.2",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "eslint-watch": "^3.1.2",
    "jest": "^21.2.0",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.0.0",
    "style-loader": "^0.21.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.8.2",
    "webpack-hot-middleware": "^2.19.1"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**",
      "!src/index.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "moduleDirectories": [
      "node_modules",
      "./"
    ],
    "moduleNameMapper": {
      "^.+.(css|scss|sass)$": "<rootDir>/__tests__/cssStub.js"
    },
    "setupFiles": [
      "raf/polyfill"
    ]
  }
}
